{
  "name": "ued",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kne/compose": "^0.1.0",
    "lottie-web": "^5.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.0"
  },
  "scripts": {
    "start": "cross-env PORT=3001 MODULES_DEV_PUBLIC_URL=/ craco start",
    "build": "cross-env COMPONENTS_NAME=ued MODULES_DEV_PUBLIC_URL=/ued  craco build",
    "build-root": "cross-env COMPONENTS_NAME=ued MODULES_DEV_PUBLIC_URL=/  craco build",
    "test": "craco test --coverage",
    "eject": "react-scripts eject",
    "init": "npm i --legacy-peer-deps",
    "create": "modules-dev-create",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@common/(.*)": "<rootDir>/src/common/$1",
      "^@components/(.*)": "<rootDir>/src/components/$1",
      "^@utils/(.*)": "<rootDir>/src/common/utils/$1"
    }
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@kne/craco-module-federation": "^1.1.2",
    "@kne/modules-dev": "^2.0.3",
    "@kne/react-fetch": "^1.4.2",
    "@kne/remote-loader": "^1.2.3",
    "@loadable/component": "^5.16.3",
    "antd": "^5.13.3",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "lodash": "^4.17.21",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.8",
    "react-router-dom": "^6.21.3"
  }
}
